extends ../layout
block content
	.container
		.row
			table.table.table-hover.table-bordered
				thead
					tr
						th 名字
						th 时间
						th 密码
				tbody
					each item in users
						tr(class="item-id-#{item._id}")
							td #{item.name}
							td #{moment(item.meta.updateAt).format('YYYY/MM/DD')}
							td #{item.password}